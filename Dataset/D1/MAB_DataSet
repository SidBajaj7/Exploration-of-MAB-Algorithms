        data.append({'trial': trial, 'arm': arm, 'reward': reward, 'cost': cost})\n","\n","    df = pd.DataFrame(data)\n","\n","    print(f\"Dataset generated with {num_arms} arms and {num_trials} trials\")\n","    print(f\"Scale parameters (means) of arms: {scales}\")\n","    print(f\"Costs for each arm: {costs}\")\n","\n","    if save_path:\n","        df.to_csv(save_path, index=False)\n","        print(f\"Dataset saved to {save_path}\")\n","\n","    return df\n","\n","# Parameters for the exponential distribution scenario\n","# Using different scale parameters to create variability\n","scales = [0.2, 0.8, 0.6, 0.4, 0.5]  # Mean = scale for exponential distribution\n","costs = [0.07, 0.12, 0.11, 0.08, 0.09]\n","\n","exponential_dataset = generate_mab_exponential_dataset(\n","    num_arms=5,\n","    num_trials=200,\n","    scales=scales,\n","    costs=costs,\n","    seed=123,\n","    save_path=\"/content/drive/MyDrive/Business Analytics/Case2_Exponential_Distribution.csv\"\n",")\n","\n","# Visualize the exponential distributions with a pastel color palette\n","plt.figure(figsize=(12, 8))\n","colors = ['#8dd3c7', '#ffffb3', '#fb8072', '#bebada', '#80b1d3']\n","for arm in range(exponential_dataset['arm'].nunique()):\n","    arm_data = exponential_dataset[exponential_dataset['arm'] == arm]['reward']\n","    cost = exponential_dataset[exponential_dataset['arm'] == arm]['cost'].iloc[0]\n","    plt.hist(arm_data, alpha=0.6, bins=30, color=colors[arm],\n","             label=f'Arm {arm} (Cost: {cost:.2f})', edgecolor='white', linewidth=0.5)\n","\n","plt.title('Reward Distributions by Arm - Exponential Scenario', fontsize=16)\n","plt.xlabel('Reward', fontsize=14)\n","plt.ylabel('Frequency', fontsize=14)\n","plt.legend(fontsize=12)\n","plt.grid(True, alpha=0.3)\n","plt.tight_layout()\n","plt.savefig('/content/drive/MyDrive/Business Analytics/case2_exponential_reward_distributions.png', dpi=300)\n","plt.show()\n","\n","# Print summary statistics for each arm\n","print(\"\\nSummary statistics for each arm:\")\n","for arm in range(exponential_dataset['arm'].nunique()):\n","    arm_data = exponential_dataset[exponential_dataset['arm'] == arm]['reward']\n","    cost = exponential_dataset[exponential_dataset['arm'] == arm]['cost'].iloc[0]\n","    expected_reward = scales[arm]  # Mean of exponential distribution equals its scale parameter\n","    net_reward = expected_reward - cost\n","    print(f\"Arm {arm}: Expected Reward = {expected_reward:.4f}, Empirical Mean = {arm_data.mean():.4f}, Cost = {cost:.2f}, Net Reward = {net_reward:.4f}\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":951},"id":"ga1PWJqFneAE","executionInfo":{"status":"ok","timestamp":1747206474528,"user_tz":-330,"elapsed":1619,"user":{"displayName":"Sidharth Bajaj (B22CH034)","userId":"10738906048579695134"}},"outputId":"268a0814-fef6-4f8d-ab75-d976326e3a7e"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Dataset generated with 5 arms and 200 trials\n","Scale parameters (means) of arms: [0.2, 0.8, 0.6, 0.4, 0.5]\n","Costs for each arm: [0.07, 0.12, 0.11, 0.08, 0.09]\n","Dataset saved to /content/drive/MyDrive/Business Analytics/Case2_Exponential_Distribution.csv\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x800 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["\n","Summary statistics for each arm:\n","Arm 0: Expected Reward = 0.2000, Empirical Mean = 0.1770, Cost = 0.07, Net Reward = 0.1300\n","Arm 1: Expected Reward = 0.8000, Empirical Mean = 0.8084, Cost = 0.12, Net Reward = 0.6800\n","Arm 2: Expected Reward = 0.6000, Empirical Mean = 0.6498, Cost = 0.11, Net Reward = 0.4900\n","Arm 3: Expected Reward = 0.4000, Empirical Mean = 0.4133, Cost = 0.08, Net Reward = 0.3200\n","Arm 4: Expected Reward = 0.5000, Empirical Mean = 0.4899, Cost = 0.09, Net Reward = 0.4100\n"]}]},{"cell_type":"code","source":[],"metadata":{"id":"A8On_tkIoLj5"},"exe